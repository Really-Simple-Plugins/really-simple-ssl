{
  "name": "cwd",
  "description": "Easily get the CWD (current working directory) of a project based on package.json, optionally starting from a given path. (node.js/javascript util)",
  "version": "0.10.0",
  "homepage": "https://github.com/jonschlinkert/cwd",
  "author": "Jon Schlinkert (https://github.com/jonschlinkert)",
  "repository": "jonschlinkert/cwd",
  "bugs": {
    "url": "https://github.com/jonschlinkert/cwd/issues"
  },
  "license": "MIT",
  "files": [
    "index.js"
  ],
  "main": "index.js",
  "engines": {
    "node": ">=0.8"
  },
  "scripts": {
    "test": "mocha"
  },
  "dependencies": {
    "find-pkg": "^0.1.2",
    "fs-exists-sync": "^0.1.0"
  },
  "devDependencies": {
    "gulp-format-md": "^0.1.9",
    "mocha": "^2.5.3",
    "normalize-path": "^2.0.1"
  },
  "keywords": [
    "current",
    "cwd",
    "dir",
    "directory",
    "file",
    "filepath",
    "path",
    "paths",
    "process",
    "process.cwd",
    "resolve",
    "resolved",
    "working"
  ],
  "verb": {
    "related": {
      "list": [
        "global-modules",
        "global-paths",
        "global-prefix",
        "is-absolute",
        "is-relative",
        "relative"
      ]
    },
    "toc": false,
    "layout": "default",
    "tasks": [
      "readme"
    ],
    "plugins": [
      "gulp-format-md"
    ],
    "lint": {
      "reflinks": true
    },
    "reflinks": [
      "verb"
    ]
  }
}
